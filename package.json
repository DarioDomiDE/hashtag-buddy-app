    "@angular/http": "8.0.0-beta.10",
    "@angular/platform-browser": "~8.2.0",
    "@angular/platform-browser-dynamic": "~8.2.0",
    "@angular/router": "~8.2.0",
    "codelyzer": "^5.1.0",
    "nativescript": "^6.0.2",
    "nativescript-angular": "^8.2.2",
    "nativescript-background-http": "^4.2.0",
    "nativescript-clipboard": "^1.2.0",
    "nativescript-exit": "^1.0.1",
    "nativescript-imagepicker": "^7.1.0",
    "nativescript-localize": "^4.2.0",
    "nativescript-localstorage": "^2.0.1",
    "nativescript-ngx-fonticon": "^5.0.2",
    "nativescript-purchase": "^2.0.13",
    "nativescript-social-share": "^1.5.2",
    "nativescript-theme-core": "^2.0.24",
    "nativescript-toast": "^2.0.0",
    "nativescript-ui-autocomplete": "^5.1.1",
    "nativescript-ui-calendar": "^5.0.0",
    "nativescript-ui-chart": "^6.0.0",
